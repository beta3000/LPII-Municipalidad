#
# Este archivo es parámetro y su valor de los SQL
# que se utlizan para crear combobox de dichas tablas
#

SQL_DISTRITO = select idDistrito,nombreDistrito from distrito
SQL_CONDICION = select cod_con,nom_con from tb_condicion
SQL_GRADO = select cod_gra_aca,nom_gra_aca from tb_grado_academico
SQL_TIPO = select cod_tipo,nom_tipo from tb_tipo_empleado
SQL_ESTADO= select idEstadoUsuario,descripcionEstadoUsuario from estadoUsuario
SQL_CARGO = select idTipoUsuario, descripcionTipoUsuario from tipousuario where idTipoUsuario <> 1
SQL_AREA = select idAreaMunicipal,nombreAreaMunicipal from areamunicipal
